ostCategory === 'kõik') {\r\n        category = null\r\n    } else {\r\n        category = postCategory\r\n    }\r\n\r\n    useEffect(() => {        \r\n        dispatch(loading())\r\n        findPosts({params: {type, userType, category, categoryType}})\r\n            .then(res => {\r\n                const data = res.data?.posts;\r\n                const sortedData = data.sort((a, b) => new Date(a) < new Date(b) ? 1 : -1);\r\n                setPosts(sortedData)\r\n                dispatch(finishedLoading())\r\n            })            \r\n    },[type, userType, category, categoryType]) \r\n    \r\n    \r\n    return (\r\n        <Grid className={styles.grid} container spacing={1}> \r\n            {loadingState === true ? \r\n                <p>Loading...</p> \r\n                :                 \r\n                <>\r\n                    {posts.length === 0 ? \r\n                    <h3>Nothing to show yet</h3>\r\n                    : \r\n                    posts.map(post => (                        \r\n                        <Grid key={post._id} item >\r\n                            <Card data={post}/>                            \r\n                        </Grid>\r\n                    ))} \r\n                </>\r\n            }                    \r\n        </Grid>            \r\n    )\r\n}\r\n\r\nexport default Content\r\n","C:\\Users\\andre\\Desktop\\MERN stack\\Wörkbook-beta1.3\\client\\src\\components\\Main\\SideBar.js",[],"C:\\Users\\andre\\Desktop\\MERN stack\\Wörkbook-beta1.3\\client\\src\\actions\\profile.js",[],"C:\\Users\\andre\\Desktop\\MERN stack\\Wörkbook-beta1.3\\client\\src\\components\\Main\\TabBar.js",[],"C:\\Users\\andre\\Desktop\\MERN stack\\Wörkbook-beta1.3\\client\\src\\components\\Comments\\AddComment.js",[],"C:\\Users\\andre\\Desktop\\MERN stack\\Wörkbook-beta1.3\\client\\src\\components\\Comments\\Comment.js",[],"C:\\Users\\andre\\Desktop\\MERN stack\\Wörkbook-beta1.3\\client\\src\\components\\Main\\Header.js",[],"C:\\Users\\and