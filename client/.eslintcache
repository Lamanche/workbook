n        if (loggedIn === true) {\r\n            history.replace('/main')}\r\n        },[])    \r\n    \r\n    return (\r\n        <div className={styles.homeContainer}>\r\n            <div className={styles.textContainer}>\r\n                <h3>Otsid või pakud teenust? Vajad, et keegi sind koolitaks või hoopis tahad oma oskusi teistega jagada? Soovid midagi välja rentida?</h3>\r\n                <h3>Alusta siit!</h3>\r\n            </div>            \r\n            <div className={styles.choicesContainer}>\r\n                <Fab onClick={looking} className={styles.lookingBtn} variant=\"extended\">\r\n                    <NavigationIcon />\r\n                    Otsin\r\n                </Fab>\r\n                <Fab onClick={offering} className={styles.offeringBtn} variant=\"extended\">\r\n                    <NavigationIcon />\r\n                    Pakun\r\n                </Fab>\r\n            </div>\r\n            <div className={styles.loginContainer}>\r\n                <Button variant='contained' color='primary' onClick={login} >Log In</Button>\r\n                <Button variant='contained' color='primary' onClick={register} >Register</Button>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Home\r\n","C:\\Users\\andre\\Desktop\\MERN stack\\Wörkbook-beta1.3\\client\\src\\components\\Main\\Main.js",[],"C:\\Users\\andre\\Desktop\\MERN stack\\Wörkbook-beta1.3\\client\\src\\api\\index.js",[],"C:\\Users\\andre\\Desktop\\MERN stack\\Wörkbook-beta1.3\\client\\src\\components\\Main\\SideBarItem.js",[],"C:\\Users\\andre\\Desktop\\MERN stack\\Wörkbook-beta1.3\\client\\src\\actions\\posts.js",[],"C:\\Users\\andre\\Desktop\\MERN stack\\Wörkbook-beta1.3\\client\\src\\components\\Auth\\helperFunc.js",[],"C:\\Users\\andre\\Desktop\\MERN stack\\Wörkbook-beta1.3\\client\\src\\actions\\location.js",[],"C:\\Users\\andre\\Desktop\\MERN stack\\Wörkbook-beta1.3\\client\\src\\components\\Auth\\styles.js",[],"C:\\Users\\andre\\Desktop\\MERN stack\\Wörkb